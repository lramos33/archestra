    "@sentry/electron": "^6.11.0",
    "@shikijs/transformers": "^3.12.2",
    "@tanstack/react-router": "^1.131.44",
    "@tanstack/react-router-devtools": "^1.131.44",
    "@vitejs/plugin-react": "^5.0.2",
    "ai": "^5.0.44",
    "better-sqlite3": "^12.2.0",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
