{
  "name": "Archestra",
  "productName": "Archestra",
  "version": "0.0.5",
  "description": "Enterprise MCP platform for AI Agents",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/archestra-ai/archestra"
  },
  "homepage": "https://archestra.ai",
  "keywords": [
    "MCP",
    "Model Context Protocol",
    "AI agents",
    "Enterprise AI",
    "Secure Runtime",
    "Prompt Injection Prevention"
  ],
  "private": true,
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "ELECTRON_RUN_AS_NODE=1 pnpm exec electron ./node_modules/vitest/dist/cli.js --config vite.main.config.ts",
    "test:e2e:packaged": "wdio run ./wdio.conf.ts",
    "prettier": "ELECTRON_RUN_AS_NODE=1 pnpm exec electron ./node_modules/prettier/bin/prettier.cjs --write .",
    "db": "ELECTRON_RUN_AS_NODE=1 pnpm exec electron ./node_modules/drizzle-kit/bin.cjs",
    "codegen:all": "pnpm codegen:libpod && pnpm codegen:archestra:api && pnpm codegen:archestra:catalog && pnpm prettier",
    "codegen:libpod": "ELECTRON_RUN_AS_NODE=1 pnpm exec electron ./node_modules/@hey-api/openapi-ts/bin/index.cjs --file openapi/libpod/openapi-ts.config.ts && npx tsx ./openapi/libpod/fix-type-generation.ts",
    "codegen:archestra:api": "ELECTRON_RUN_AS_NODE=1 CODEGEN=1 pnpm exec electron ./node_modules/tsx/dist/cli.mjs openapi/archestra/api/generate-openapi.ts && ELECTRON_RUN_AS_NODE=1 pnpm exec electron ./node_modules/@hey-api/openapi-ts/bin/index.cjs --file openapi/archestra/api/openapi-ts.config.ts",
    "codegen:archestra:catalog": "ELECTRON_RUN_AS_NODE=1 pnpm exec electron ./node_modules/@hey-api/openapi-ts/bin/index.cjs --file openapi/archestra/catalog/openapi-ts.config.ts"
  },
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
